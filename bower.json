{
  "name": "gsg-theme-bootstrap-blog",
  "version": "1.0.0",
  "homepage": "https://github.com/belgac/gsg-theme-bootstrap-blog",
  "authors": [
    "belgac <hans.scheuren@bizliner.com>"
  ],
  "description": "The gulp-stat-gen bootstrap theme blog inspired by http://getbootstrap.com/examples/blog/ by @mdo",
  "keywords": [
    "gsg-theme"
  ],
  "license": "MIT",
  "main": [
    "assets/css/local.css",
    "templates/default.html",
    "templates/index.html",
    "templates/journal.html",
    "templates/partials/about-side.html",
    "templates/partials/css.html",
    "templates/partials/header.html",
    "templates/partials/journal-pager.html",
    "templates/partials/nav.html",
    "templates/partials/post-summary.html",
    "templates/partials/scripts.html",
    "templates/partials/tag-list.html"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "gsg-theme-default": "~1.0.3"
  },
  "overrides": {
    "gsg-theme-default":{
      "main": [
        "templates/page.html",
        "templates/post.html",
        "templates/tag.html",
        "templates/partials/footer.html",
        "templates/partials/head.html"
      ]
    }
  }
}
